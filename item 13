#grados Farenhit a Celcius
print("BIENVENIDO")
print("PUEDES CONVERTIR GRADOS fahrenheit A celsius")
def fahrenheit_celsius(f):
    
    return (f - 32) * 5.0/9.0
n1 = float(input("ingrese los grados fahrenheit_"))
print("--------------------")
print("RESULTADO EN GRADOS celsius")
print(fahrenheit_celsius(n1)) 

